{
  "id": null,
  "uid": "pred-oracle-perf",
  "title": "Prediction Oracle: Scores, Signals, Fills",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "tags": ["prediction-oracle", "trading"],
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Model Score (rolling)",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(prediction_oracle_model_score{deployment=\"prod\"}[5m])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.2 },
              { "color": "green", "value": 0.35 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Signals emitted",
      "datasource": { "type": "prometheus", "uid": "PROMETHEUS_DS" },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(prediction_oracle_signals_total{deployment=\"prod\"}[5m])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0.5 },
              { "color": "green", "value": 2 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Fill latency p95 (seconds)",
      "datasource": { "type": "prometheus", "uid": "PROMETHEUS_DS" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(prediction_oracle_fill_latency_seconds_bucket{deployment=\"prod\"}[5m])) by (le))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 2 },
              { "color": "red", "value": 4 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "deployment",
        "type": "query",
        "label": "Deployment",
        "datasource": { "type": "prometheus", "uid": "PROMETHEUS_DS" },
        "query": "label_values(prediction_oracle_model_score, deployment)",
        "current": { "text": "prod", "value": "prod" },
        "refresh": 1
      }
    ]
  }
}
